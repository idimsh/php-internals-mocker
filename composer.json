{
    "name": "idimsh/php-internals-mocker",
    "type": "library",
    "description": "Helps mocking PHP internal functions calls",
    "keywords": [
        "PHP",
        "idimsh",
        "php-internals-mocker"
    ],
    "homepage": "https://github.com/idimsh/php-internals-mocker",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulrahman Dimashki",
            "email": "idimsh@example.com",
            "homepage": "https://github.com/idimsh",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0 <8",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "idimsh\\PhpInternalsMocker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "idimsh\\PhpInternalsMockerTest\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
